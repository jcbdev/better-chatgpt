{
  "name": "chatgpt-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/chatgpt-api/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/chatgpt-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/chatgpt-api/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/chatgpt-api",
        "tsConfig": "packages/chatgpt-api/tsconfig.lib.json",
        "packageJson": "packages/chatgpt-api/package.json",
        "main": "packages/chatgpt-api/src/index.ts",
        "assets": ["packages/chatgpt-api/*.md"]
      }
    }
  },
  "tags": ["scope:public", "type:util", "target:all"]
}
